<!DOCTYPE html>
<html>
<head>
    <title>
        <%= config.title %>
    </title>
    <meta charset="UTF-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1"/>
    <script>
        if (("flysay.com" == window.location.host) && (window.location.protocol != "https:"))
            window.location.protocol = "https";
    </script>
    <%- css(['css/bootstrap.min.css']) %>
    <%- js(['js/jquery-2.1.4.min.js', 'js/jquery.pjax.min.js']) %>
    <%- css(['js/highlightjs/styles/tomorrow.css']) %>
    <%- css(['css/base.css']) %>
</head>
<body>
<div id="pjax-container">
    <%- partial('_partial/header') %>
    <%- body %>
    <%- partial('_partial/footer') %>
</div>
<%- js(['js/highlightjs/highlight.pack.js', 'js/base.js']) %>
</body>
</html>
