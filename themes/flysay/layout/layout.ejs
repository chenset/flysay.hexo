<!DOCTYPE html>
<html>
<head>
    <title>
        <%= config.title %>
    </title>
    <meta charset="UTF-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1"/>
    <%- css(['css/bootstrap.min.css']) %>
    <%- js(['js/jquery-2.1.4.min.js', 'js/jquery.pjax.min.js']) %>
    <%- css(['js/highlightjs/styles/tomorrow.css']) %>
    <%- css(['css/base.css']) %>
    <%- css(['css/nprogress.css']) %>
    <% if(is_home()){ %>
    <link rel="canonical" href="<%= config.url %>"/>
    <% }else{ %>
    <link rel="canonical" href="<%= config.url + url_for(path) %>"/>
    <% } %>
</head>
<body>
<div id="pjax-container">
    <%- partial('_partial/header') %>
    <%- body %>
    <%- partial('_partial/footer') %>
</div>
<div class="hvr-ripple-out" style="position:fixed;bottom: 50px;right:50px;width: 40px;height: 40px;background: #dfdfdf;border-radius: 50%">
</div>
<%- js(['js/highlightjs/highlight.pack.js', 'js/nprogress.js', 'js/base.js']) %>
</body>
</html>
