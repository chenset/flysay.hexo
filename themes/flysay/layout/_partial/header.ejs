<header class="main-header container-fluid">
    <div class="container">
        <div class="row">
            <div class="col-md-12 text-right">
                <nav>
                    <ul>
                        <% for (var i in theme.menu){ %>
                        <li><a href="<%- url_for(theme.menu[i]) %>"><%= i %></a></li>
                        <% } %>
                    </ul>
                </nav>
            </div>
        </div>
    </div>
</header>
