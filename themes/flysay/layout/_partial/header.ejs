<header class="main-header">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <nav>
                    <strong class="main-logo-wrap">
                        <a id="main-logo"
                                href="<%- url_for('/') %>">FlySay - Chenset</a></strong>
                    <ul>
                        <% for (var i in theme.menu){ %>
                        <li><a href="<%- url_for(theme.menu[i]) %>"><%= i %></a></li>
                        <% } %>
                    </ul>
                </nav>

                <%- search_form({}) %>

            </div>
        </div>
    </div>
</header>
