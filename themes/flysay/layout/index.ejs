<div class="container content-wrap">

    <% page.posts.each(function(page){ %>
    <%- partial('_partial/article', {post: page, index: true}) %>
    <% }) %>

    <% if (page.prev || page.next){ %>
    <nav class="page-nav">
        <%- paginator({
            prev_text: '上一页',
            next_text: '下一页'
        }) %>
    </nav>
    <% } %>

</div>